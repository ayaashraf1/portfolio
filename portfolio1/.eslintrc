{
    "rules": {
        "react/react-in-jsx-scope": "off",
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension": [0],
        "import/extensions": "off"
    },
    "extends": [ "airbnb","eslint:recommended","plugin:jest/recommended","plugin:cypress/recommended","plugin:react/recommended","prettier" ],
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "globals": {
        "document": false
    },
    "plugins": ["react", "jest","cypress"]
}